<div class="row">
  {{#each products}}
  <div class="col-sm-6 col-md-3">
    <div class="thumbnail">
      <img src="{{ this.imagePath }}" alt="..." class="img-responsive">
      <div class="caption">
        <h3>{{ this.title }}</h3>
        <img src="/home/billele/babysshow/public/images/012mois/bodie.jpg" alt="">
        <p class="description">{{ this.description }}</p>
        <div class="clearfix">
          <div class="price pull-left">{{ this.price }}</div>
          <p><a href="#" class="pull-right btn btn-default " role="button">Voir</a></p>
        </div>
      </div>
    </div>
  </div>
  {{/each}}
</div>
